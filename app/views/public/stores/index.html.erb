<div class="container">
  <div class="category-header mb-3">
    <nav>
      <ul style="list-style: none;">
        <% StoreCategory.all.each do |store_category| %>
          <li><h5><%= link_to store_category.name, stores_path(store_category_id: store_category.id), class: 'text-dark' %></h5></li>
        <% end %>
      </ul>
    </nav>
  </div>
  <%= render 'store_index', stores: @stores %>
</div>